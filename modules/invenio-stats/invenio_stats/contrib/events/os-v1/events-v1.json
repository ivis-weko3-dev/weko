{
  "index_patterns": [
    "__SEARCH_INDEX_PREFIX__events-stats-index-*"
  ],
  "settings": {
    "index": {
      "refresh_interval": "5s",
      "lifecycle.name": "weko_stats_policy",
      "lifecycle.rollover_alias": "__SEARCH_INDEX_PREFIX__events-stats-index"
    }
  },
  "mappings": {
    "dynamic_templates": [
      {
        "date_fields": {
          "match_mapping_type": "date",
          "mapping": {
            "type": "date",
            "format": "date_optional_time"
          }
        }
      }
    ],
    "date_detection": false,
    "dynamic": false,
    "numeric_detection": false,
    "properties": {
      "timestamp": {
        "type": "date",
        "format": "strict_date_hour_minute_second"
      },
      "event_type": {
        "type": "keyword"
      },

      "task_id": {
        "type": "keyword",
        "index": true
      },
      "task_name": {
        "type": "keyword",
        "index": true
      },
      "unique_id": {
        "type": "keyword",
        "index": true
      },
      "unique_session_id": {
        "type": "keyword",
        "index": true
      },
      "visitor_id": {
        "type": "keyword",
        "index": true
      },
      "country": {
        "type": "keyword",
        "index": true
      },
      "task_state": {
        "type": "keyword",
        "index": true
      },
      "start_time": {
        "type": "keyword",
        "index": true
      },
      "end_time": {
        "type": "keyword",
        "index": true
      },
      "total_records": {
        "type": "integer"
      },
      "repository_name": {
        "type": "keyword",
        "index": true
      },
      "execution_time": {
        "type": "keyword",
        "index": true
      },
      "is_robot": {
        "type": "boolean"
      },
      "is_restricted": {
        "type": "boolean"
      },

      "size": {
        "type": "double"
      },
      "updated_timestamp": {
        "type": "date"
      },

      "item_id": {
        "type": "keyword",
        "index": true
      },
      "item_title": {
        "type": "keyword",
        "index": true
      },
      "bucket_id": {
        "type": "keyword",
        "index": true
      },
      "file_id": {
        "type": "keyword",
        "index": true
      },
      "root_file_id": {
        "type": "keyword",
        "index": true
      },
      "file_key": {
        "type": "keyword",
        "index": true
      },
      "collection": {
        "type": "keyword",
        "index": true
      },
      "referrer": {
        "type": "keyword",
        "index": true
      },
      "is_billing_item": {
        "type": "boolean"
      },
      "billing_file_price": {
        "type": "keyword",
        "index": true
      },
      "accessrole": {
        "type": "keyword",
        "index": true
      },
      "userrole": {
        "type": "keyword",
        "index": true
      },
      "site_license_name": {
        "type": "keyword",
        "index": true
      },
      "site_license_flag": {
        "type": "boolean"
      },
      "index_list": {
        "type": "keyword",
        "index": true
      },
      "cur_user_id": {
        "type": "keyword",
        "index": true
      },
      "user_group_list": {
        "type": "nested",
        "properties": {
          "group_id": {
            "type": "keyword"
          },
          "group_name": {
            "type": "keyword",
            "index": true
          }
        }
      },
      "hostname": {
        "type": "keyword",
        "index": true
      },
      "remote_addr": {
        "type": "keyword",
        "index": true
      },

      "username": {
        "type": "keyword",
        "index": true
      },
      "pid_type": {
        "type": "keyword",
        "index": true
      },
      "pid_value": {
        "type": "keyword",
        "index": true
      },
      "record_name": {
        "type": "keyword",
        "index": true
      },

      "record_id": {
        "type": "keyword"
      },

      "labels": {
        "type": "keyword",
        "index": true
      },
      "search_detail": {
        "properties": {
          "search_key": {
            "type": "keyword",
            "index": true
          },
          "search_type": {
            "type": "keyword",
            "index": true
          }
        }
      }
    }
  },
  "aliases": {
    "__SEARCH_INDEX_PREFIX__events-stats-celery-task": {
      "filter": {
        "term": {
          "event_type": "celery-task"
        }
      }
    },
    "__SEARCH_INDEX_PREFIX__events-stats-file-download": {
      "filter": {
        "term": {
          "event_type": "file-download"
        }
      }
    },
    "__SEARCH_INDEX_PREFIX__events-stats-file-preview": {
      "filter": {
        "term": {
          "event_type": "file-preview"
        }
      }
    },
    "__SEARCH_INDEX_PREFIX__events-stats-item-create": {
      "filter": {
        "term": {
          "event_type": "item-create"
        }
      }
    },
    "__SEARCH_INDEX_PREFIX__events-stats-record-view": {
      "filter": {
        "term": {
          "event_type": "record-view"
        }
      }
    },
    "__SEARCH_INDEX_PREFIX__events-stats-search": {
      "filter": {
        "term": {
          "event_type": "search"
        }
      }
    },
    "__SEARCH_INDEX_PREFIX__events-stats-top-view": {
      "filter": {
        "term": {
          "event_type": "top-view"
        }
      }
    }
  }
}
